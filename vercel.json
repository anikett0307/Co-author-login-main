{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend-react/vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend-react/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend-react/dist/$1"
    }
  ]
}
